<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ymlion.pluginuninstalled">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Plugin1Activity">
        </activity>
        <activity
            android:name=".Plugin2Activity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".Plugin3Activity">
        </activity>

        <receiver android:name=".Plugin1Receiver">
            <intent-filter>
                <action android:name="com.ymlion.pluginuninstalled.PLUGIN_RECEIVER_1"/>
            </intent-filter>
        </receiver>

        <service android:name=".Plugin1Service">
        </service>

        <activity
            android:name=".Plugin4Activity"
            android:label="@string/title_activity_plugin4"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>